def multiplication_table(n):
    total_list  = []
    for i in range(1,n+1):
        for j in range(1,n+1):
            m = int(i*j)
            total_list.append(m)
    multiplication_table = []
    for i in range(0,len(total_list),n):
            multiplication_table.append(total_list[i:i+n])
    return multiplication_table
